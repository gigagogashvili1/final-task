{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/db-module/(.*)": "<rootDir>/../libs/db-module/src/$1",
    "@app/db-module": "<rootDir>/../libs/db-module/src",
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/auth": "<rootDir>/../libs/auth/src",
    "@app/db-lib/(.*)": "<rootDir>/../libs/db-lib/src/$1",
    "@app/db-lib": "<rootDir>/../libs/db-lib/src",
    "@app/auth-lib/(.*)": "<rootDir>/../libs/auth-lib/src/$1",
    "@app/auth-lib": "<rootDir>/../libs/auth-lib/src",
    "@app/common-lib/(.*)": "<rootDir>/../libs/common-lib/src/$1",
    "@app/common-lib": "<rootDir>/../libs/common-lib/src"
  }
}